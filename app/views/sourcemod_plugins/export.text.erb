// Exported from SourceMod Translator
// http://<%= request.env['HTTP_HOST'] %><%= url_for(@sourcemod_plugin) %>
//
"Phrases"
{
    <% @sourcemod_plugin.phrases.each do |phrase| %>
    "<%= phrase.name %>"
    {
        <% if phrase.format %>
        <% phrase.format_infos.has_description.each do |fmt| %>
        // <%= fmt.position %>: <%= fmt.description %>
        <% end %>
        "#format"   "<%= phrase.format %>"
        <% end %>
        <% phrase.translations.each do |translation| %>
        "<%= translation.language.iso_code %>"        "<%= translation.text %>"
        <% end %>
    }

    <% end %>
}